<%= form_for [@user, @project], remote: true do |f| %>
<h1>New Project</h1>
  <div class="field"> 
	  <%= f.label :title, "Title: " %>
	  <%= f.text_field :title %>
	</div></br>
  <div class="field">	  
  	<%= f.label :objective, "Objective: " %>
	  <%= f.text_field :objective %>
	</div></br>
  <div class="date_field">
	  <div class="field">			
	  	<%= f.label :due_date %>
			<%= f.date_field :due_date %>
		 </div></br>
	</div>
	<div class="action">
		<%= f.submit "Get Started", class: "medium button success round", id: "submit_button", remote: true %>
	</div>
	<div class="action">
		<%= link_to "Back to your page", '#' %></p>
	</div>
	</div>
<% end %>