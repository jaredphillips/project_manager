<h1>New Project</h1>
<%= form_for [@user, @project] do |f| %>
  <div class="field"> 
	  <%= f.label :title, "Title: " %>
	  <%= f.text_field :title %>
	</div>
	<div class="field"> 
	  <%= f.label :objective, "Objective: " %>
	  <%= f.text_field :objective %>
	</div>
	<div class="field"> 
	  <%= f.label :team, "Team: " %>
	  <%= f.text_field :team %>
	</div>
	<div class="field"> 
	  <%= f.label :task, "Task: " %>
	  <%= f.text_field :task %>
	</div>
  
  <div class="date_field">
		<div class="field">
			<%= f.label :due_date %>
			<%= f.date_field :due_date %>
	  </div>
	</div>

	<div class="action">
		<%= f.submit "Get Started" %>
	</div>
<% end %>
