<div class="center">
	<div class="page-header">
		<h4>Dashboard<small> Current Projects on the go.</small></h4>
	</div>
	<div class="projects" id="incomplete_projects">
		<% @projects.each do |project| %>
			<div class="page-header">
			  <h4><%= link_to project.title, user_project_path(@user, project) %> | <small><%= project.objective %></small> | <small><%= project.due_date %></small></h4>
			</div>
		<% end %>
	</div>
</div>

